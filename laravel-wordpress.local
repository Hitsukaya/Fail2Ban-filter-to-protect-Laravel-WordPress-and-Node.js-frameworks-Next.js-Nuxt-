  [laravel-wordpress]
  enabled  = true
  port     = http,https
  filter   = laravel-wordpress
  logpath  = /var/log/nginx/access.log /var/log/nginx/error.log
  maxretry = 1
  bantime  = -1
  banaction = iptables-multiport
  findtime = 600
  ignoreip = 127.0.0.1/8 ::1 <YOUR-IP-HERE>
  persistent = true
