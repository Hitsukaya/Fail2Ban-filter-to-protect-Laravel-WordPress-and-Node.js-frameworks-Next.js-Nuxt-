[nginx-ssl-handshake-protection]
enabled   = true
port      = http,https
filter    = nginx-ssl-handshake-protection
logpath   = /var/log/nginx/error.log
maxretry  = 3
findtime  = 3600
bantime   = -1
action 	  = iptables-multiport[name=nginx-ssl-handshake-protection, port="http,https", protocol=tcp]

